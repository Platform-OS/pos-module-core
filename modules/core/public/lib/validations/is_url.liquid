{% comment %}
  params: @url
          @field_name
          @key[optional]
{% endcomment %}

{% liquid
  assign key = key | default: 'modules/core/validation.not_url'
  assign is_url = url | matches: '^https?:\/\/[\S]+'

  if is_url != true
    function c = 'modules/core/helpers/register_error', contract: c, field_name: field_name, key: key
  endif
  
  return c
%}  